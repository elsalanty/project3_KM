const Quiz = require('../models/quiz');

module.exports = {
    createUser(req,res){
        console.log('create user method called');
        const userProps = req.body;
        console.log('user values',userProps);
        Quiz.create(userProps).then(user=>{
            console.log('record created in the db',user);
            res.send(user);
        })
    }
}